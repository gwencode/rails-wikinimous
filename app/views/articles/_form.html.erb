<%= form_with model: @article do |f| %>
  <%= f.label :title, class: 'form-label' %>
  <%= f.text_field :title, class: 'form-text' %>
  <br>
  <br>
  <%= f.label :content, class: 'form-label' %>
  <%= f.text_field :content, class: 'form-text' %>
  <br>
  <br>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
